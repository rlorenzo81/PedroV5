            {
              "pluginID": "com.bylazar.capture",
              "widgetID": "Capture"
            }
          ]
        },
        {
          "x": 11,
          "y": 0,
          "w": 5,
          "h": 9,
          "widgets": [
            {
              "pluginID": "com.bylazar.field",
              "widgetID": "Field"
